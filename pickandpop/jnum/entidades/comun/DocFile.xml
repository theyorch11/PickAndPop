<?xml version="1.0" encoding="UTF-8"?>
<entidad xmlns="http://jnum.com/entidad" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jnum.com/entidad
	../../../workingset/esquemas/entidad.xsd">
	<modulo>generico</modulo>
	<nombre>docfile</nombre>
	<nombreTabla>docfile</nombreTabla>
	<modulo>generico</modulo>
	<label>Fichero</label>
	<comboLabel>{filename}</comboLabel>
	<atributos>

		<atributo>
			<nombre>filename</nombre>
			<nombreColumna>filename</nombreColumna>
			<longitud>100</longitud>
			<obligatorio>true</obligatorio>
			<label>Nombre Fichero</label>
			<clase>java.lang.String</clase>
			<esComboLabel>true</esComboLabel>
			<edicion>
				<tipo>text</tipo>
				<longitud>100</longitud>
				<validaciones>
				</validaciones>
			</edicion>
			<secciones>
				<busqueda>
					<operador>ilike</operador>
				</busqueda>
				<listado>true</listado>
				<insercion>true</insercion>
				<modificacion>true</modificacion>
			</secciones>
		</atributo>
		<atributo>
			<nombre>filetype</nombre>
			<nombreColumna>filetype</nombreColumna>
			<longitud>100</longitud>
			<obligatorio>true</obligatorio>
			<label>Tipo</label>
			<clase>java.lang.String</clase>
			<edicion>
				<tipo>text</tipo>
				<longitud>100</longitud>
				<validaciones>
				</validaciones>
			</edicion>
			<secciones>
				<busqueda>
					<operador>ilike</operador>
				</busqueda>
				<listado>true</listado>
				<insercion>true</insercion>
				<modificacion>true</modificacion>
			</secciones>
		</atributo>
		<atributo>
			<nombre>fichero</nombre>
			<nombreColumna>fichero</nombreColumna>
			<longitud>20000</longitud>
			<obligatorio>false</obligatorio>
			<label>Fichero</label>
			<clase>java.lang.String</clase>
			<edicion>
				<tipo>lob</tipo>
				<longitud>15</longitud>
				<validaciones>
				</validaciones>
			</edicion>
			<secciones>
				<busqueda>
					<operador>ilike</operador>
				</busqueda>
				<listado>true</listado>
				<insercion>true</insercion>
				<modificacion>true</modificacion>
			</secciones>
		</atributo>
	</atributos>

	<detalles>
	</detalles>
	<visual>
		<busqueda>
			<agregable>true</agregable>
			<vista>
				<definicion>
					void.filenameilike;
					void.filetypeilike
				</definicion>
			</vista>
		</busqueda>
		<adicion>
			<agregable>true</agregable>
			<vista>
				<definicion>
					this.filename;
					this.filetype;
					this.fichero
				</definicion>
			</vista>
		</adicion>
		<edicion>
			<editable>true</editable>
			<vista>
				<definicion>
					this.filename;
					this.filetype;
					this.fichero
				</definicion>
			</vista>
		</edicion>
		<listado>
			<deletable>true</deletable>
			<vista>
				<definicion>filename,filetype</definicion>
			</vista>
		</listado>
	</visual>	

	<menuLabel>Fichero</menuLabel>
	<nombreID>id_fichero</nombreID>
</entidad>
