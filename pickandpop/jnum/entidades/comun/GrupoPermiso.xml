<?xml version="1.0" encoding="UTF-8"?>
<entidad xmlns="http://jnum.com/entidad"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jnum.com/entidad ../../esquemas/entidad.xsd">

	<modulo>seguridad</modulo>
	<nombre>grupoPermiso</nombre>
	<nombreTabla>grupo_permiso</nombreTabla>
	<nombreID>id_grupo_permiso</nombreID>
	<label>Grupo Permiso</label>
	<menuLabel>Grupos de Permisos</menuLabel>
	<menuOrden>30</menuOrden>
	<comboLabel>{nombre}</comboLabel>
	<atributos>
		<atributo>
			<nombre>nombre</nombre>
			<nombreColumna>NO_GP_PERMISO</nombreColumna>
			<obligatorio>true</obligatorio>
			<label>nombre</label>
			<clase>java.lang.String</clase>
			<unico>true</unico>
			<esComboLabel>true</esComboLabel>
			<longitud>50</longitud>
			<edicion>
				<tipo>text</tipo>
				<validaciones>
					<validacion>
						<nombre>requiredFieldValidator</nombre>
						<parametros>
							<parametro>
								<nombre>highlight</nombre>
								<valor>true</valor>
							</parametro>
							<parametro>
								<nombre>display</nombre>
								<valor>none</valor>
							</parametro>
						</parametros>
					</validacion>
				</validaciones>
			</edicion>
			<secciones>
				<insercion>true</insercion>
				<busqueda>
					<operador>ilike</operador>
				</busqueda>
				<listado>true</listado>
				<modificacion>true</modificacion>
			</secciones>
		</atributo>
	</atributos>
	
	<detalles>
		<detalle>
			<nombre>permiso</nombre>
			<nombreTablaRelacion>permiso_grupopermiso</nombreTablaRelacion>
			<atributo>permisos</atributo>
			<atributoDetalle>gruposPermiso</atributoDetalle>
			<tipoRelacion>many-to-many</tipoRelacion>
			<propietario>true</propietario>
			<fetchType>EAGER</fetchType>
			<edicion>
				<tipo>detalleSelectSearch</tipo>
			</edicion>
			<label>Permisos</label>
			<listado>
				<atributos>
					<atributo>
						<nombre>nombre</nombre>
					</atributo>
					<atributo>
						<nombre>descripcion</nombre>
					</atributo>
				</atributos>
			</listado>
		</detalle>
		
		<detalle>
			<nombre>rol</nombre>
			<nombreTablaRelacion>grupopermiso_roles</nombreTablaRelacion>
			<atributo>roles</atributo>
			<atributoDetalle>gruposPermiso</atributoDetalle>
			<propietario>false</propietario>
			<tipoRelacion>many-to-many</tipoRelacion>
			<edicion>
				<tipo>detalleSelect</tipo>
			</edicion>
			<label>Roles</label>
			<listado>
				<atributos>
					<atributo>
						<nombre>nombre</nombre>
					</atributo>
				</atributos>
			</listado>
		</detalle>
	</detalles>
	<visual>
		<busqueda>
			<agregable>true</agregable>
			<vista>
				<definicion>
					void.nombreilike
				</definicion>
			</vista>
		</busqueda>
		<adicion>
			<agregable>true</agregable>
			<vista>
				<definicion>
					this.nombre
				</definicion>
			</vista>
		</adicion>
		<edicion>
			<editable>true</editable>
			<vista>
				<definicion>
				this.nombre
				</definicion>
			</vista>
		</edicion>
		<listado>
			<deletable>true</deletable>
			<vista>
				<definicion>nombre</definicion>
			</vista>
		</listado>
	</visual>	
</entidad>
