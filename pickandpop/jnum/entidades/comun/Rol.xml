<?xml version="1.0" encoding="UTF-8"?>
<entidad xmlns="http://jnum.com/entidad"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jnum.com/entidad ../../esquemas/entidad.xsd">

	<modulo>seguridad</modulo>
	<nombre>rol</nombre>
	<nombreTabla>rol</nombreTabla>
	<label>rol</label>
	<modulo>seguridad</modulo>
	<menuLabel>Roles</menuLabel>
	<nombreID>id_rol</nombreID>
	<comboLabel>{nombre}</comboLabel>
	<atributos>
		<atributo>
			<nombre>nombre</nombre>
			<nombreColumna>nombre</nombreColumna>
			<obligatorio>true</obligatorio>
			<label>nombre</label>
			<clase>java.lang.String</clase>
			<longitud>50</longitud>
			<unico>true</unico>
			<esComboLabel>true</esComboLabel>
			<edicion>
				<tipo>text</tipo>
				<validaciones>
				</validaciones>
			</edicion>
			<secciones>
				<insercion>true</insercion>
				<busqueda>
					<operador>eq</operador>
				</busqueda>
				<listado>true</listado>
				<modificacion>true</modificacion>
			</secciones>
		</atributo>
	</atributos>

	<detalles>
		<detalle>
			<nombre>user</nombre>
			<atributo>usuarios</atributo>
			<atributoDetalle>roles</atributoDetalle>
			<nombreTablaRelacion>usuario_roles</nombreTablaRelacion>
			<tipoRelacion>many-to-many</tipoRelacion>
			<propietario>true</propietario>
			<edicion>
				<tipo>detalleSelect</tipo>
			</edicion>
			<label>usuarios</label>
		</detalle>
		<detalle>
			<nombre>grupoPermiso</nombre>
			<nombreTablaRelacion>grupopermiso_roles</nombreTablaRelacion>
			<atributo>gruposPermiso</atributo>
			<atributoDetalle>roles</atributoDetalle>
			<tipoRelacion>many-to-many</tipoRelacion>
			<propietario>true</propietario>
			<fetchType>EAGER</fetchType>
			<edicion>
				<tipo>detalleSelect</tipo>
			</edicion>
			<label>Grupos de Permisos</label>
			<listado>
				<atributos>
					<atributo>
						<nombre>nombre</nombre>
					</atributo>
				</atributos>
			</listado>
		</detalle>		
	</detalles>

	<visual>
		<busqueda>
			<agregable>true</agregable>
			<vista>
				<definicion>
					void.nombreeq
				</definicion>
			</vista>
		</busqueda>
		<adicion>
			<agregable>true</agregable>
			<vista>
				<definicion>
					this.nombre
				</definicion>
			</vista>
		</adicion>
		<edicion>
			<editable>true</editable>
			<vista>
				<definicion>
				this.nombre
				</definicion>
			</vista>
		</edicion>
		<listado>
			<deletable>true</deletable>
			<vista>
				<definicion>nombre</definicion>
			</vista>
		</listado>
	</visual>
</entidad>
